	<element name="Funds">		
		<complexType>
			<sequence>		
				<element name="Name" type="string"/>
				<element name="Ticker" type="string"/>
				<element name="InceptionDate" type="string"/>	
			</sequence>		
		</complexType>
	</element>
	
	<element name="Securities">
		<complexType>
			<sequence>		
